---
layout: layouts/base/index.njk
templateClass: tmpl-architect
---

{{ architect.name }}

<img src="{% imageUrlFor architect.image.asset._ref, 400 %}" alt="{{ architect.image.asset.alt }}" />

{{ architect.body | safe }}

<h2>Routes</h2>
<ul>
  {% for route in architect.routes %}
    <li><a href="/routes/{{route.slug }}">{{ route.title }}</a></li>
  {% endfor %}
</ul>
