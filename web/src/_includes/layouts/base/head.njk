<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ title or page.title or renderData.title or metadata.title }} | {{ metadata.title }}</title>

    <style>
      @font-face {
        font-family: 'Jost';
        src: url('/static/fonts/jost.woff2') format('woff2 supports variations'),
            url('/static/fonts/jost.woff2') format('woff2-variations');
        font-weight: 100 900;
        font-display: swap;
      }
    </style>

    <meta name="Description" content="{{ description or renderData.description or metadata.description }}">
    <link rel="canonical" href="{{ env.url }}{{ page.url }}" />
    <meta property="og:title" content="{{ title or renderData.title or metadata.title }}" />
    <meta property="og:url" content="{{ page.url }}" />
    <meta property="og:description" content="{{ description or renderData.description }}" />
    <meta property="og:image" content="/static/img/logo.png" />
    <meta property="og:type" content="article" />

    <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
    <link rel="manifest" href="/site.webmanifest">
    <link rel="mask-icon" href="/safari-pinned-tab.svg" color="#5bbad5">
    <meta name="msapplication-TileColor" content="#da532c">
    <meta name="theme-color" content="#ffffff">

    <link rel="stylesheet" href="{{ hash['/css/styles.css'] | url }}"/>
  </head>
