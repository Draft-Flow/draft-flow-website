<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ title or renderData.title or metadata.title }} | {{ metadata.title }}</title>
    <meta name="Description" content="{{ description or renderData.description or metadata.description }}">
    <link rel="canonical" href="{{ env.url }}{{ page.url }}" />
    <meta property="og:title" content="{{ title or renderData.title or metadata.title }}" />
    <meta property="og:url" content="{{ page.url }}" />
    <meta property="og:description" content="{{ description or renderData.description }}" />
    <meta property="og:image" content="/static/img/logo.png" />
    <meta property="og:type" content="article" />
      {# <link rel="alternate" href="{{ metadata.feed.path | url }}" type="application/atom+xml" title="{{ metadata.title }}"> #}
    {% set css %}
      {% include "style.css" %}
    {% endset %}
    <style>
      {{ css | cssmin | safe }}
    </style>
  </head>